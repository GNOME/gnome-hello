#: gnome-hello.desktop.in.h:1
msgid "GNOME Hello Demo"
msgstr "GNOME Hello 示範"

#: gnome-hello.desktop.in.h:2
msgid "Sample application using GNOME technologies"
msgstr "用來示範 GNOME 技術的範例程式"

#. ** gnomehello-widgets
#: src/app.c:45
msgid "Gnome Hello"
msgstr "GNOME Hello"

#: src/app.c:51 src/menus.c:127
msgid "Hello, World!"
msgstr "您好！"

#: src/app.c:92
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "無法分析指定位置大小的字串‘%s’"

#: src/app.c:101
msgid "Special Greetings to:\n"
msgstr "特別問候：\n"

#: src/hello.c:50
msgid "Say hello to specific people listed on the command line"
msgstr "向在指定列裡指定的人打招呼"

#: src/hello.c:59
msgid "Specify a message other than \"Hello, World!\""
msgstr "指定代替「您好！」的訊息。"

#: src/hello.c:60
msgid "MESSAGE"
msgstr "訊息"

#: src/hello.c:68
msgid "Specify the geometry of the main window"
msgstr "指定主視窗的尺寸及位置"

#: src/hello.c:69
msgid "GEOMETRY"
msgstr "位置大小"

#: src/hello.c:129
msgid "You must specify someone to greet."
msgstr "閣下必須指定想打招呼的人。"

#: src/hello.c:133
msgid "Command line arguments are only allowed with --greet."
msgstr "指令列參數只能配合 --greet 一起使用。"

#: src/menus.c:35
msgid "_New Hello"
msgstr "新增訊息(_N)"

#: src/menus.c:36 src/menus.c:85
msgid "Create a new hello"
msgstr "新增訊息"

#: src/menus.c:85
msgid "New"
msgstr "新增"

#: src/menus.c:89
msgid "Prev"
msgstr "上一個"

#: src/menus.c:89
msgid "Previous hello"
msgstr "上一個訊息"

#: src/menus.c:90
msgid "Next"
msgstr "下一個"

#: src/menus.c:90
msgid "Next hello"
msgstr "下一個訊息"

#: src/menus.c:116
msgid "This does nothing; it is only a demonstration."
msgstr "它沒有實際作用，只是一個示範。"

#: src/menus.c:174
msgid "GNOME Hello"
msgstr "GNOME Hello"

#: src/menus.c:177
msgid "A sample GNOME application."
msgstr "一個 GNOME 的範例程式。"
