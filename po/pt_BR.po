#: gnome-hello.desktop.in.h:1
msgid "GNOME Hello Demo"
msgstr "Demonstração GNOME Hello"

#: gnome-hello.desktop.in.h:2
msgid "Sample application using GNOME technologies"
msgstr "Aplicação de exemplo usando as tecnologias GNOME"

#. ** gnomehello-widgets
#: src/app.c:45
msgid "Gnome Hello"
msgstr "Olá Gnome"

#: src/app.c:51 src/menus.c:127
msgid "Hello, World!"
msgstr "Olá, Mundo!"

#: src/app.c:92
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "Incapaz de analisar expressão de geometria `%s'"

#: src/app.c:101
msgid "Special Greetings to:\n"
msgstr "Agradecimentos Especiais para:\n"

#: src/hello.c:50
msgid "Say hello to specific people listed on the command line"
msgstr "Diz olá para pessoas especificas, listadas na linha de comando"

#: src/hello.c:59
msgid "Specify a message other than \"Hello, World!\""
msgstr "Especificar uma mensagem diferente de \"Olá Mundo!\""

#: src/hello.c:60
msgid "MESSAGE"
msgstr "MENSAGEM"

#: src/hello.c:68
msgid "Specify the geometry of the main window"
msgstr "Especificar a geometria da janela principal"

#: src/hello.c:69
msgid "GEOMETRY"
msgstr "GEOMETRIA"

#: src/hello.c:129
msgid "You must specify someone to greet."
msgstr "Você tem que especificar alguém para cumprimentar."

#: src/hello.c:133
msgid "Command line arguments are only allowed with --greet."
msgstr "Argumentos de linha de comando são permitidos apenas com --greet."

#: src/menus.c:35
msgid "_New Hello"
msgstr "_Novo Olá"

#: src/menus.c:36 src/menus.c:85
msgid "Create a new hello"
msgstr "Criar um novo olá"

#: src/menus.c:85
msgid "New"
msgstr "Novo"

#: src/menus.c:89
msgid "Prev"
msgstr "Anterior"

#: src/menus.c:89
msgid "Previous hello"
msgstr "Olá anterior"

#: src/menus.c:90
msgid "Next"
msgstr "Seguinte"

#: src/menus.c:90
msgid "Next hello"
msgstr "Olá seguinte"

#: src/menus.c:116
msgid "This does nothing; it is only a demonstration."
msgstr "Isto não faz nada; é apenas uma demonstração."

#: src/menus.c:174
msgid "GNOME Hello"
msgstr "GNOME Hello"

#: src/menus.c:177
msgid "A sample GNOME application."
msgstr "Uma aplicação de demonstração do GNOME"
