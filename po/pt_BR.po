#: src/hello.c:49
msgid "Say hello to specific people listed on the command line"
msgstr "Diz olá para pessoas especificas, listadas na linha de comando"

#: src/hello.c:58
msgid "Specify a message other than \"Hello, World!\""
msgstr "Especificar uma mensagem diferente de \"Olá Mundo!\""

#: src/hello.c:59
msgid "MESSAGE"
msgstr "MENSAGEM"

#: src/hello.c:67
msgid "Specify the geometry of the main window"
msgstr "Especificar a geometria da janela principal"

#: src/hello.c:68
msgid "GEOMETRY"
msgstr "GEOMETRIA"

#: src/hello.c:120
msgid "You must specify someone to greet."
msgstr "Você tem que especificar alguém para cumprimentar."

#: src/hello.c:124
msgid "Command line arguments are only allowed with --greet."
msgstr "Argumentos de linha de comando são permitidos apenas com --greet."

