#. ** gnomehello-widgets
#: src/app.c:45
msgid "Gnome Hello"
msgstr "Gnome Ahoj"

#: src/app.c:51 src/menus.c:127
msgid "Hello, World!"
msgstr "Ahoj, světe!"

#: src/app.c:92
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "Nemohu zpracovat řetězec geometrie `%s'"

#: src/app.c:101
msgid "Special Greetings to:\n"
msgstr "Speciální pozdravy pro:\n"

#: src/hello.c:50
msgid "Say hello to specific people listed on the command line"
msgstr "Řekněte ahoj konkrétním lidem vypsaným na příkazové řádce"

#: src/hello.c:59
msgid "Specify a message other than \"Hello, World!\""
msgstr "Zadejte zprávu jinou než \"Ahoj, světe!\""

#: src/hello.c:60
msgid "MESSAGE"
msgstr "ZPRÁVA"

#: src/hello.c:68
msgid "Specify the geometry of the main window"
msgstr "Zadejte geometrii hlavního okna"

#: src/hello.c:69
msgid "GEOMETRY"
msgstr "GEOMETRIE"

#: src/hello.c:129
msgid "You must specify someone to greet."
msgstr "Musíte určit někoho, koho pozdravit."

#: src/hello.c:133
msgid "Command line arguments are only allowed with --greet."
msgstr "Argumenty příkazové řádky jsou povoleny jen s --greet."

#: src/menus.c:35
msgid "_New Hello"
msgstr "_Nový pozdrav"

#: src/menus.c:36 src/menus.c:85
