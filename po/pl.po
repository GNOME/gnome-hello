#: gnome-hello.desktop.in.h:1
msgid "GNOME Hello Demo"
msgstr "Demo GNOME Hello"

#: gnome-hello.desktop.in.h:2
msgid "Sample application using GNOME technologies"
msgstr "Przykładowa aplikacja używająca technologii GNOME"

#. ** gnomehello-widgets
#: src/app.c:45
msgid "Gnome Hello"
msgstr "Gnome Hello"

#: src/app.c:51 src/menus.c:127
msgid "Hello, World!"
msgstr "Witaj świecie!"

#: src/app.c:92
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "Nie można rozkodować ciągu '%s' określającego wymiary"

#: src/app.c:101
msgid "Special Greetings to:\n"
msgstr "Specjalne pozdrowienia dla:\n"

#: src/hello.c:50
msgid "Say hello to specific people listed on the command line"
msgstr "Pozdrów osoby podane w wierszu poleceń"

#: src/hello.c:59
msgid "Specify a message other than \"Hello, World!\""
msgstr "Określ wiadomość inną niż domyśle \"Witaj świecie!\""

#: src/hello.c:60
msgid "MESSAGE"
msgstr "WIADOMOŚĆ"

#: src/hello.c:68
msgid "Specify the geometry of the main window"
msgstr "Określ wymiary głównego okna"

#: src/hello.c:69
msgid "GEOMETRY"
msgstr "WYMIARY"

#: src/hello.c:129
msgid "You must specify someone to greet."
msgstr "Należy podać listę pozdrawianych osób."

#: src/hello.c:133
msgid "Command line arguments are only allowed with --greet."
msgstr "Argumenty w wierszu poleceń są dozwolone tylko z opcją --greet."

#: src/menus.c:35
msgid "_New Hello"
msgstr "Nowe pozdrowienie"

#: src/menus.c:36 src/menus.c:85
msgid "Create a new hello"
msgstr "Tworzy nowe pozdrowienie"

#: src/menus.c:85
msgid "New"
msgstr "Nowe"

#: src/menus.c:89
msgid "Prev"
msgstr "Poprzednie"

#: src/menus.c:89
msgid "Previous hello"
msgstr "Poprzednie pozdrowienie"

#: src/menus.c:90
msgid "Next"
msgstr "Następne"

#: src/menus.c:90
msgid "Next hello"
msgstr "Następne pozdrowienie"

#: src/menus.c:116
msgid "This does nothing; it is only a demonstration."
msgstr "Nic się nie stało. To tylko demonstracja."

#: src/menus.c:174
msgid "GNOME Hello"
msgstr "GNOME Hello"

#: src/menus.c:177
msgid "A sample GNOME application."
msgstr "Przykładowa aplikacja dla GNOME."

