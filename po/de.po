#: ../gnome-hello.desktop.in.h:1
msgid "GNOME Hello Demo"
msgstr "GNOME Hallo-Demo"

#: ../gnome-hello.desktop.in.h:2 ../src/menus.c:195
msgid "Sample application using GNOME technologies"
msgstr "Beispiel-Anwendung zur Demonstration der GNOME-Technologien"

#. Set the human-readable name for the application
#: ../src/app.c:52 ../src/hello.c:103
msgid "GNOME Hello"
msgstr "GNOME Hallo"

#: ../src/app.c:74 ../src/menus.c:129
msgid "Hello, World!"
msgstr "Hallo, Welt!"

#: ../src/app.c:91
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "Die Geometriezeichenkette »%s« konnte nicht verarbeitet werden"

#: ../src/app.c:98
msgid "Special Greetings to:\n"
msgstr "Besondere Grüße an:\n"

#: ../src/hello.c:42
msgid "Say hello to specific people listed on the command line"
msgstr "Die in der Befehlszeile angegebenen Personen grüßen"

#: ../src/hello.c:51
msgid "Specify a message other than \"Hello, World!\""
msgstr "Eine andere Nachricht als »Hallo, Welt!« verwenden"

#: ../src/hello.c:52
msgid "MESSAGE"
msgstr "NACHRICHT"

#: ../src/hello.c:60
msgid "Specify the geometry of the main window"
msgstr "Die Geometrie des Hauptfensters"

#: ../src/hello.c:61
msgid "GEOMETRY"
msgstr "GEOMETRIE"

#: ../src/hello.c:91
msgid "- GNOME Hello"
msgstr "- GNOME Hallo"

#: ../src/hello.c:96
#, c-format
msgid ""
"%s\n"
"Run '%s --help' to see a full list of available command line options.\n"
msgstr ""
"%s\n"
"Rufen Sie %s --help« auf, um eine vollständige Liste der\n"
"verfügbaren Befehlszeilenoptionen zu sehen.\n"

#: ../src/hello.c:121
#, c-format
msgid "You must specify someone to greet.\n"
msgstr "Sie müssen eine zu grüßende Person angeben.\n"

#: ../src/hello.c:126
#, c-format
msgid "Command line arguments are only allowed with --greet.\n"
msgstr "Befehlszeilenargumente sind nur bei der Option --greet zulässig.\n"

