#: gnome-hello.desktop.in.h:1
msgid "GNOME Hello Demo"
msgstr "GNOME Ahoj Demo"

#: gnome-hello.desktop.in.h:2
msgid "Sample application using GNOME technologies"
msgstr "Príklad aplikácie používajúcej technológiu GNOME"

#. ** gnomehello-widgets
#: src/app.c:45
msgid "Gnome Hello"
msgstr "Gnome ahoj"

#: src/app.c:51 src/menus.c:127
msgid "Hello, World!"
msgstr "Ahoj, Svet!"

#: src/app.c:92
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "Nemôžem rozdeliť reťazec geometrie `%s'"

#: src/app.c:101
msgid "Special Greetings to:\n"
msgstr "Špeciálne pozdravy pre:\n"

#: src/hello.c:50
msgid "Say hello to specific people listed on the command line"
msgstr "Povedať ahoj pre ľudí zadaných v príkazovom riadku"

#: src/hello.c:59
msgid "Specify a message other than \"Hello, World!\""
msgstr "Zadajte správu odlišnú od \"Ahoj, Svet!\""

#: src/hello.c:60
msgid "MESSAGE"
msgstr "SPRÁVA"

#: src/hello.c:68
msgid "Specify the geometry of the main window"
msgstr "Zadajte geometriu hlavného okna"

#: src/hello.c:69
msgid "GEOMETRY"
msgstr "GEOMETRIA"

#: src/hello.c:129
msgid "You must specify someone to greet."
msgstr "Musíte zadať niekoho, koho chcete pozdraviť."

#: src/hello.c:133
msgid "Command line arguments are only allowed with --greet."
msgstr "Argumenty príkazového riadku sú povolené iba s --greet."

#: src/menus.c:35
msgid "_New Hello"
msgstr "_Nové ahoj"

#: src/menus.c:36 src/menus.c:85
msgid "Create a new hello"
msgstr "Vytvoriť nové ahoj"

#: src/menus.c:85
msgid "New"
msgstr "Nový"

#: src/menus.c:89
msgid "Prev"
msgstr "Predchádzajúci"

#: src/menus.c:89
msgid "Previous hello"
msgstr "Predchádzajúce ahoj"

#: src/menus.c:90
msgid "Next"
msgstr "Ďalší"

#: src/menus.c:90
msgid "Next hello"
msgstr "Ďalšie ahoj"

#: src/menus.c:116
msgid "This does nothing; it is only a demonstration."
msgstr "Toto nevykonáva nič, je to iba ukážka."

#: src/menus.c:174
msgid "GNOME Hello"
msgstr "GNOME Ahoj"

#: src/menus.c:177
msgid "A sample GNOME application."
msgstr "Príklad GNOME aplikácie."

