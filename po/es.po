#: gnome-hello.desktop.in.h:1
msgid "GNOME Hello Demo"
msgstr "Demostración de GNOME Hola"

#: gnome-hello.desktop.in.h:2
msgid "Sample application using GNOME technologies"
msgstr "Aplicación de ejemplo del uso de las tencnologías de GNOME"

#. ** gnomehello-widgets
#: src/app.c:45
msgid "Gnome Hello"
msgstr "Gnome Hola"

#: src/app.c:51 src/menus.c:127
msgid "Hello, World!"
msgstr "¡Hola, Mundo!"

#: src/app.c:92
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "No pude analizar la cadena de texto de geometría «%s»"

#: src/app.c:101
msgid "Special Greetings to:\n"
msgstr "Saludos especiales a:\n"

#: src/hello.c:50
msgid "Say hello to specific people listed on the command line"
msgstr "Mandar un saludo a la lista de personas dada en la línea de comando"

#: src/hello.c:59
msgid "Specify a message other than \"Hello, World!\""
msgstr "Especificar un mensaje distinto de «¡Hola, Mundo!»"

#: src/hello.c:60
msgid "MESSAGE"
msgstr "MENSAJE"

#: src/hello.c:68
msgid "Specify the geometry of the main window"
msgstr "Especificar la geometría de la ventana principal"

#: src/hello.c:69
msgid "GEOMETRY"
msgstr "GEOMETRIA"

#: src/hello.c:129
msgid "You must specify someone to greet."
msgstr "Debe especificar una persona a quien saludar."

#: src/hello.c:133
msgid "Command line arguments are only allowed with --greet."
msgstr "Los argumentos en la línea de comando solo son válidos con --greet."

#: src/menus.c:35
msgid "_New Hello"
msgstr "_Nuevo Saludo"

#: src/menus.c:36 src/menus.c:85
msgid "Create a new hello"
msgstr "Crea un nuevo saludo"

#: src/menus.c:85
msgid "New"
msgstr "Nuevo"

#: src/menus.c:89
msgid "Prev"
msgstr "Anterior"

#: src/menus.c:89
msgid "Previous hello"
msgstr "Saludo anterior"

#: src/menus.c:90
msgid "Next"
msgstr "Siguiente"

#: src/menus.c:90
msgid "Next hello"
msgstr "Saludo siguiente"

#: src/menus.c:116
msgid "This does nothing; it is only a demonstration."
msgstr "Esto no hace nada; es solo una muestra."

#: src/menus.c:174
msgid "GNOME Hello"
msgstr "GNOME Hola"

#: src/menus.c:177
msgid "A sample GNOME application."
msgstr "Una muestra de programa para GNOME."
