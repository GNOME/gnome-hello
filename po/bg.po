#: gnome-hello.desktop.in.h:1
msgid "GNOME Hello Demo"
msgstr "GNOME Здравей Демо"

#: gnome-hello.desktop.in.h:2
msgid "Sample application using GNOME technologies"
msgstr "Примерно приложение използващо GNOME технологии"

#. ** gnomehello-widgets
#: src/app.c:45
msgid "Gnome Hello"
msgstr "GNOME здравей"

#: src/app.c:51 src/menus.c:127
msgid "Hello, World!"
msgstr "Здравей, Свят!"

#: src/app.c:92
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "Не успях да анализирам низа за геометрията `%s'"

#: src/app.c:101
msgid "Special Greetings to:\n"
msgstr "Специални поздрави за:\n"

#: src/hello.c:50
msgid "Say hello to specific people listed on the command line"
msgstr "Кажи здравей на хората изброени на командния ред"

#: src/hello.c:59
msgid "Specify a message other than \"Hello, World!\""
msgstr "Използвай друго съобщение вместо \"Здравей, Свят!\""

#: src/hello.c:60
msgid "MESSAGE"
msgstr "СЪОБЩЕНИЕ"

#: src/hello.c:68
msgid "Specify the geometry of the main window"
msgstr "Укажи геометрията на главния прозорец"

#: src/hello.c:69
msgid "GEOMETRY"
msgstr "ГЕОМЕТРИЯ"

#: src/hello.c:129
msgid "You must specify someone to greet."
msgstr "Трябва да укажете някой, когото да поздравите."

#: src/hello.c:133
msgid "Command line arguments are only allowed with --greet."
msgstr "Атрибутите на командния ред се приемат единствено с --greet."

#: src/menus.c:35
msgid "_New Hello"
msgstr "_Ново Здравей"

#: src/menus.c:36 src/menus.c:85
msgid "Create a new hello"
msgstr "Създава ново Здравей"

#: src/menus.c:85
msgid "New"
msgstr "Нов"

#: src/menus.c:89
msgid "Prev"
msgstr "Пред"

#: src/menus.c:89
msgid "Previous hello"
msgstr "Предишно Здравей"

#: src/menus.c:90
msgid "Next"
msgstr "Следв"

#: src/menus.c:90
msgid "Next hello"
msgstr "Следващо Здравей"

#: src/menus.c:116
msgid "This does nothing; it is only a demonstration."
msgstr "Това не прави нищо, а е само демонстрация."

#: src/menus.c:174
msgid "GNOME Hello"
msgstr "GNOME Здравей"

#: src/menus.c:177
msgid "A sample GNOME application."
msgstr "Примерно Гном приложение"
