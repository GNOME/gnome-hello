#: gnome-hello.desktop.in.h:1
msgid "GNOME Hello Demo"
msgstr "Gnomen Hei -esimerkki"

#: gnome-hello.desktop.in.h:2
msgid "Sample application using GNOME technologies"
msgstr "Gnome-teknologiaa käyttävä esimerkkisovellus"

#. ** gnomehello-widgets
#: src/app.c:45
msgid "Gnome Hello"
msgstr "Gnomen Hei"

#: src/app.c:51 src/menus.c:127
msgid "Hello, World!"
msgstr "Hei, maailma!"

#: src/app.c:92
#, c-format
msgid "Could not parse geometry string `%s'"
msgstr "Mittamääritystä \"%s\" ei voitu tulkita."

#: src/app.c:101
msgid "Special Greetings to:\n"
msgstr "Erikoistervehdykset seuraaville:\n"

#: src/hello.c:50
msgid "Say hello to specific people listed on the command line"
msgstr "Sano \"hei\" komentorivillä listatuille ihmisille"

#: src/hello.c:59
msgid "Specify a message other than \"Hello, World!\""
msgstr "Määrittele muu viesti kuin \"Hei, maailma!\""

#: src/hello.c:60
msgid "MESSAGE"
msgstr "VIESTI"

#: src/hello.c:68
msgid "Specify the geometry of the main window"
msgstr "Määrittele pääikkunan mitat"

#: src/hello.c:69
msgid "GEOMETRY"
msgstr "MITAT"

#: src/hello.c:129
msgid "You must specify someone to greet."
msgstr "Sinun täytyy määritellä joku, jota tervehtiä."

#: src/hello.c:133
msgid "Command line arguments are only allowed with --greet."
msgstr "Komentoriviargumentit ovat sallittuja vain valitsimell --greet."

#: src/menus.c:35
msgid "_New Hello"
msgstr "_Uusi \"Hei\""

#: src/menus.c:36 src/menus.c:85
msgid "Create a new hello"
msgstr "Luo uusi \"hei\""

#: src/menus.c:85
msgid "New"
msgstr "Uusi"

#: src/menus.c:89
msgid "Prev"
msgstr "Edellinen"

#: src/menus.c:89
msgid "Previous hello"
msgstr "Edellinen \"hei\""

#: src/menus.c:90
msgid "Next"
msgstr "Seuraava"

#: src/menus.c:90
msgid "Next hello"
msgstr "Seuraava \"hei\""

#: src/menus.c:116
msgid "This does nothing; it is only a demonstration."
msgstr "Tämä ei tee mitään, onpahan vain demonstraatio."

#: src/menus.c:174
msgid "GNOME Hello"
msgstr "Gnomen Hei"

#: src/menus.c:177
msgid "A sample GNOME application."
msgstr "Esimerkinomainen Gnome-sovellus."

